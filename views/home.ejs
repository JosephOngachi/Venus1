    <!-- JavaScript for the Timer -->
    <script>
        // Function to format remaining time in days, hours, and minutes
        function formatTimer(remainingTime) {
            var days = Math.floor(remainingTime / (1000 * 60 * 60 * 24));
            var hours = Math.floor((remainingTime % (1000 * 60 * 60 * 24)) / (1000 * 60 * 60));
            var minutes = Math.floor((remainingTime % (1000 * 60 * 60)) / (1000 * 60));
            return (days > 0 ? days + " days " : "") + (hours > 0 ? hours + " hours " : "") + (minutes > 0 ? minutes + " minutes " : "");
        }

        // Update timer display
        function updateTimer() {
            var timerElement = document.getElementById("timer");
            var remainingTime = <%= remainingTime %>; // Remaining time in milliseconds
            var formattedTime = formatTimer(remainingTime);
            timerElement.textContent = formattedTime;

            // Update remaining time every second
            setInterval(function() {
                remainingTime -= 1000; // Subtract 1 second
                formattedTime = formatTimer(remainingTime);
                timerElement.textContent = formattedTime;
            }, 1000);
        }

        // Start the timer
        updateTimer();
    </script>
